{
  "extends": "airbnb/base",
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-console": [2],
    "object-curly-spacing": [0],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "id-length": [
      2,
      {
        "exceptions": [
          "_",
          "$"
        ]
      }
    ]
  }
}
